{
  "name": "neo-scraper",
  "version": "0.7.2",
  "description": "Scrape a booru-like post object from the DOM.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "test": "cross-env JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest",
    "test:ci": "cross-env JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest --colors --silent",
    "test:debug": "cross-env JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs HEADLESS=false jest --testTimeout=100000000",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src scripts",
    "summary-table": "npm run build && node --experimental-specifier-resolution=node --loader ts-node/esm scripts/summary-table.ts"
  },
  "author": "neobooru",
  "license": "MIT",
  "dependencies": {
    "js-cookie": "^3.0.5",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^26.0.14",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/js-cookie": "^3.0.3",
    "@types/jsdom": "^12.2.4",
    "@types/puppeteer": "^5.4.4",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cross-env": "^7.0.3",
    "eslint": "8.7.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^26.6.3",
    "jest-puppeteer": "^6.0.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "jsdom": "^15.2.1",
    "prettier": "2.5.1",
    "puppeteer": "^13.1.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "exports": {
    ".": "./dist/index.js",
    "./": "./dist/"
  },
  "jest": {
    "preset": "./jest.preset.cjs",
    "resolver": "jest-ts-webcompat-resolver",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testTimeout": 15000,
    "testRegex": "^.+\\.test\\.ts$",
    "setupFilesAfterEnv": [
      "./src/test/jest.setup.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    }
  }
}
